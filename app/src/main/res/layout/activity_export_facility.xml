<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_65"
        android:background="@color/mainColor">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginTop="@dimen/dp_20">

            <ImageView
                android:id="@+id/add"
                android:layout_width="@dimen/dp_20"
                android:layout_height="@dimen/dp_20"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/dp_10"
                android:layout_marginTop="@dimen/dp_20"
                android:src="@drawable/xui_ic_return_back" />

            <TextView
                android:id="@+id/title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:gravity="center"
                android:text="迁出"
                android:textColor="@color/white1"
                android:textSize="17sp" />
        </RelativeLayout>
    </RelativeLayout>

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_120"
                android:gravity="center_vertical"
                android:orientation="horizontal">

                <ImageView
                    android:layout_width="@dimen/dp_41"
                    android:layout_height="@dimen/dp_32"
                    android:layout_marginLeft="@dimen/dp_12"
                    android:src="@mipmap/ic_export_selected" />

                <TextView
                    android:id="@+id/co"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/dp_20"
                    android:layout_marginRight="@dimen/dp_50"
                    android:text="已读取照片1张、视频0段共计0.0G请选择相关设备导出"
                    android:textColor="@color/color_333"
                    android:textSize="@dimen/text_size_18" />
            </LinearLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dp_1"
                    android:background="@color/main_bg" />

                <ImageView
                    android:id="@+id/iv_1"
                    android:layout_width="@dimen/dp_12"
                    android:layout_height="@dimen/dp_15"
                    android:layout_marginLeft="@dimen/dp_14"
                    android:layout_marginTop="@dimen/dp_16"
                    android:src="@mipmap/ic_sd_card" />

                <TextView
                    android:id="@+id/sd_name_1"
                    style="@style/no_font_padding_text_view"
                    android:layout_marginLeft="@dimen/dp_7"
                    android:layout_marginTop="@dimen/dp_16"
                    android:layout_toRightOf="@id/iv_1"
                    android:text="名称：usb5000"
                    android:textColor="@color/color_333"
                    android:textSize="@dimen/text_size_15" />

                <TextView
                    android:id="@+id/room_reduce_1"
                    style="@style/no_font_padding_text_view"
                    android:layout_alignParentRight="true"
                    android:layout_marginTop="@dimen/dp_16"
                    android:layout_marginRight="@dimen/dp_30"
                    android:text="已使用11G/39G"
                    android:textColor="@color/color_333"
                    android:textSize="@dimen/text_size_15" />

                <View
                    android:id="@+id/line_1"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dp_1"
                    android:layout_below="@id/room_reduce_1"
                    android:layout_marginLeft="@dimen/dp_12"
                    android:layout_marginTop="@dimen/dp_12"
                    android:layout_marginRight="@dimen/dp_12"
                    android:background="@color/main_bg" />

                <com.languang.bluebox.coustomview.CustomPercentView
                    android:id="@+id/custom_percent_view_1"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dp_25"
                    android:layout_below="@id/line_1"
                    android:layout_marginLeft="@dimen/dp_12"
                    android:layout_marginTop="@dimen/dp_16"
                    android:layout_marginRight="@dimen/dp_12" />

                <Button
                    android:id="@+id/browse_1"
                    android:layout_width="@dimen/dp_80"
                    android:layout_height="@dimen/dp_28"
                    android:layout_below="@id/custom_percent_view_1"
                    android:layout_alignParentRight="true"
                    android:layout_marginTop="@dimen/dp_15"
                    android:layout_marginRight="@dimen/dp_12"
                    android:background="@drawable/bg_button_blue"
                    android:text="写入"
                    android:textColor="@color/color_fff"
                    android:textSize="@dimen/text_size_15" />

                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dp_1"
                    android:layout_below="@id/browse_1"
                    android:layout_marginTop="@dimen/dp_14"
                    android:background="@color/main_bg" />
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp_35">

                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dp_1"
                    android:background="@color/main_bg" />

                <ImageView
                    android:id="@+id/iv_2"
                    android:layout_width="@dimen/dp_12"
                    android:layout_height="@dimen/dp_15"
                    android:layout_marginLeft="@dimen/dp_14"
                    android:layout_marginTop="@dimen/dp_16"
                    android:src="@mipmap/ic_sd_card" />

                <TextView
                    android:id="@+id/sd_name_2"
                    style="@style/no_font_padding_text_view"
                    android:layout_marginLeft="@dimen/dp_7"
                    android:layout_marginTop="@dimen/dp_16"
                    android:layout_toRightOf="@id/iv_2"
                    android:text="名称：usb6000"
                    android:textColor="@color/color_333"
                    android:textSize="@dimen/text_size_15" />

                <TextView
                    android:id="@+id/room_reduce_2"
                    style="@style/no_font_padding_text_view"
                    android:layout_alignParentRight="true"
                    android:layout_marginTop="@dimen/dp_16"
                    android:layout_marginRight="@dimen/dp_30"
                    android:text="已使用11G/39G"
                    android:textColor="@color/color_333"
                    android:textSize="@dimen/text_size_15" />

                <View
                    android:id="@+id/line_2"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dp_1"
                    android:layout_below="@id/room_reduce_2"
                    android:layout_marginLeft="@dimen/dp_12"
                    android:layout_marginTop="@dimen/dp_12"
                    android:layout_marginRight="@dimen/dp_12"
                    android:background="@color/main_bg" />

                <com.languang.bluebox.coustomview.CustomPercentView
                    android:id="@+id/custom_percent_view_2"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dp_25"
                    android:layout_below="@id/line_2"
                    android:layout_marginLeft="@dimen/dp_12"
                    android:layout_marginTop="@dimen/dp_16"
                    android:layout_marginRight="@dimen/dp_12" />

                <Button
                    android:id="@+id/browse_2"
                    android:layout_width="@dimen/dp_80"
                    android:layout_height="@dimen/dp_28"
                    android:layout_below="@id/custom_percent_view_2"
                    android:layout_alignParentRight="true"
                    android:layout_marginTop="@dimen/dp_15"
                    android:layout_marginRight="@dimen/dp_12"
                    android:background="@drawable/bg_button_blue"
                    android:text="写入"
                    android:textColor="@color/color_fff"
                    android:textSize="@dimen/text_size_15" />

                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dp_1"
                    android:layout_below="@id/browse_2"
                    android:layout_marginTop="@dimen/dp_14"
                    android:background="@color/main_bg" />
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp_35">

                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dp_1"
                    android:background="@color/main_bg" />

                <ImageView
                    android:id="@+id/iv"
                    android:layout_width="@dimen/dp_12"
                    android:layout_height="@dimen/dp_15"
                    android:layout_marginLeft="@dimen/dp_14"
                    android:layout_marginTop="@dimen/dp_16"
                    android:src="@mipmap/ic_sd_card" />

                <TextView
                    android:id="@+id/sd_name"
                    style="@style/no_font_padding_text_view"
                    android:layout_marginLeft="@dimen/dp_7"
                    android:layout_marginTop="@dimen/dp_16"
                    android:layout_toRightOf="@id/iv"
                    android:text="名称：本机--"
                    android:textColor="@color/color_333"
                    android:textSize="@dimen/text_size_15" />

                <TextView
                    android:id="@+id/room_reduce"
                    style="@style/no_font_padding_text_view"
                    android:layout_alignParentRight="true"
                    android:layout_marginTop="@dimen/dp_16"
                    android:layout_marginRight="@dimen/dp_30"
                    android:text="已使用156G/256G"
                    android:textColor="@color/color_333"
                    android:textSize="@dimen/text_size_15" />

                <View
                    android:id="@+id/line"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dp_1"
                    android:layout_below="@id/room_reduce"
                    android:layout_marginLeft="@dimen/dp_12"
                    android:layout_marginTop="@dimen/dp_12"
                    android:layout_marginRight="@dimen/dp_12"
                    android:background="@color/main_bg" />

                <com.languang.bluebox.coustomview.CustomPercentView
                    android:id="@+id/custom_percent_view"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dp_25"
                    android:layout_below="@id/line"
                    android:layout_marginLeft="@dimen/dp_12"
                    android:layout_marginTop="@dimen/dp_16"
                    android:layout_marginRight="@dimen/dp_12" />

                <Button
                    android:id="@+id/browse"
                    android:layout_width="@dimen/dp_80"
                    android:layout_height="@dimen/dp_28"
                    android:layout_below="@id/custom_percent_view"
                    android:layout_alignParentRight="true"
                    android:layout_marginTop="@dimen/dp_15"
                    android:layout_marginRight="@dimen/dp_12"
                    android:background="@drawable/bg_button_blue"
                    android:text="浏览"
                    android:textColor="@color/color_fff"
                    android:textSize="@dimen/text_size_15"
                    android:visibility="gone" />

                <Button
                    android:id="@+id/export_all"
                    android:layout_width="@dimen/dp_80"
                    android:layout_height="@dimen/dp_28"
                    android:layout_below="@id/custom_percent_view"
                    android:layout_alignParentRight="true"
                    android:layout_marginTop="@dimen/dp_15"
                    android:layout_marginRight="@dimen/dp_16"
                    android:layout_toLeftOf="@id/browse"
                    android:background="@color/color_eb6100"
                    android:text="下载"
                    android:textColor="@color/color_fff"
                    android:textSize="@dimen/text_size_15" />

                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dp_1"
                    android:layout_below="@id/export_all"
                    android:layout_marginTop="@dimen/dp_14"
                    android:background="@color/main_bg" />
            </RelativeLayout>
        </LinearLayout>
    </ScrollView>
</LinearLayout>